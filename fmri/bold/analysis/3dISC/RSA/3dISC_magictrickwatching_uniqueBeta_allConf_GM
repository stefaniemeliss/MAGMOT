3dISC -prefix ISC_magictrickwatching_uniqueBeta_allConf_GM -jobs 4 \
	 -model 'grp+uniqueBeta_allConf+grUniqueBeta_allConf+(1|Subj1)+(1|Subj2)' \
	 -qVars   'grp,uniqueBeta_allConf,grUniqueBeta_allConf' \
	 -qVarCenters   'uniqueBeta_allConf,grUniqueBeta_allConf' \
	 -gltCode ave '1 0 0 0' \
	 -gltCode G11vG22 '0 1 0 0' \
	 -gltCode G11 '1 0.5 0 0' \
	 -gltCode G22 '1 -0.5 0 0' \
	 -gltCode uniqueBeta_allConf '0 0 1 0'  \
	 -gltCode uniqueBeta_allConf1vuniqueBeta_allConf2 '0 0 0 1'\
	 -gltCode uniqueBeta_allConf1 '0 0 1 0.5'  \
	 -gltCode uniqueBeta_allConf2 '0 0 1 -0.5'  \
	 -mask /storage/shared/research/cinn/2018/MAGMOT/derivatives/ROI_masks/output/MNI_res-epi_label-GM_mask.nii.gz \
	 -dataTable @dataTable_magictrickwatching_unique.txt \