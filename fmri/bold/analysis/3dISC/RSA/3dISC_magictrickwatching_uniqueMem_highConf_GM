3dISC -prefix ISC_magictrickwatching_uniqueMem_highConf_GM -jobs 4 \
	 -model 'grp+uniqueMem_highConf+grUniqueMem_highConf+(1|Subj1)+(1|Subj2)' \
	 -qVars   'grp,uniqueMem_highConf,grUniqueMem_highConf' \
	 -qVarCenters   'uniqueMem_highConf,grUniqueMem_highConf' \
	 -gltCode ave '1 0 0 0' \
	 -gltCode G11vG22 '0 1 0 0' \
	 -gltCode G11 '1 0.5 0 0' \
	 -gltCode G22 '1 -0.5 0 0' \
	 -gltCode uniqueMem_highConf '0 0 1 0'  \
	 -gltCode uniqueMem_highConf1vuniqueMem_highConf2 '0 0 0 1'\
	 -gltCode uniqueMem_highConf1 '0 0 1 0.5'  \
	 -gltCode uniqueMem_highConf2 '0 0 1 -0.5'  \
	 -mask /storage/shared/research/cinn/2018/MAGMOT/derivatives/ROI_masks/output/MNI_res-epi_label-GM_mask.nii.gz \
	 -dataTable @dataTable_magictrickwatching_unique.txt \