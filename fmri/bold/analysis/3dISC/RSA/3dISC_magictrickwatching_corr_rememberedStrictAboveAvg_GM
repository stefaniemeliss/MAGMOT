	 -model 'grp+corr_rememberedStrictAboveAvg+grCorr_rememberedStrictAboveAvg+(1|Subj1)+(1|Subj2)' \
	 -qVars   'grp,corr_rememberedStrictAboveAvg,grCorr_rememberedStrictAboveAvg' \
	 -qVarCenters   'corr_rememberedStrictAboveAvg,grCorr_rememberedStrictAboveAvg' \
	 -gltCode ave '1 0 0 0' \
	 -gltCode G11vG22 '0 1 0 0' \
	 -gltCode G11 '1 0.5 0 0' \
	 -gltCode G22 '1 -0.5 0 0' \
	 -gltCode corr_rememberedStrictAboveAvg '0 0 1 0'  \
	 -gltCode corr_rememberedStrictAboveAvg1vcorr_rememberedStrictAboveAvg2 '0 0 0 1'\
	 -gltCode corr_rememberedStrictAboveAvg1 '0 0 1 0.5'  \
	 -gltCode corr_rememberedStrictAboveAvg2 '0 0 1 -0.5'  \
	 -mask /storage/shared/research/cinn/2018/MAGMOT/derivatives/ROI_masks/output/MNI_res-epi_label-GM_mask.nii.gz \
