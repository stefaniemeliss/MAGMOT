3dISC -prefix ISC_magictrickwatching_uniqueBeta_aboveAvgConf_GM -jobs 4 \
	 -model 'grp+uniqueBeta_aboveAvgConf+grUniqueBeta_aboveAvgConf+(1|Subj1)+(1|Subj2)' \
	 -qVars   'grp,uniqueBeta_aboveAvgConf,grUniqueBeta_aboveAvgConf' \
	 -qVarCenters   'uniqueBeta_aboveAvgConf,grUniqueBeta_aboveAvgConf' \
	 -gltCode ave '1 0 0 0' \
	 -gltCode G11vG22 '0 1 0 0' \
	 -gltCode G11 '1 0.5 0 0' \
	 -gltCode G22 '1 -0.5 0 0' \
	 -gltCode uniqueBeta_aboveAvgConf '0 0 1 0'  \
	 -gltCode uniqueBeta_aboveAvgConf1vuniqueBeta_aboveAvgConf2 '0 0 0 1'\
	 -gltCode uniqueBeta_aboveAvgConf1 '0 0 1 0.5'  \
	 -gltCode uniqueBeta_aboveAvgConf2 '0 0 1 -0.5'  \
	 -mask /storage/shared/research/cinn/2018/MAGMOT/derivatives/ROI_masks/output/MNI_res-epi_label-GM_mask.nii.gz \
	 -dataTable @dataTable_magictrickwatching_unique.txt \