	 -model 'grp+corr_rememberedStrictHigh+grCorr_rememberedStrictHigh+(1|Subj1)+(1|Subj2)' \
	 -qVars   'grp,corr_rememberedStrictHigh,grCorr_rememberedStrictHigh' \
	 -qVarCenters   'corr_rememberedStrictHigh,grCorr_rememberedStrictHigh' \
	 -gltCode ave '1 0 0 0' \
	 -gltCode G11vG22 '0 1 0 0' \
	 -gltCode G11 '1 0.5 0 0' \
	 -gltCode G22 '1 -0.5 0 0' \
	 -gltCode corr_rememberedStrictHigh '0 0 1 0'  \
	 -gltCode corr_rememberedStrictHigh1vcorr_rememberedStrictHigh2 '0 0 0 1'\
	 -gltCode corr_rememberedStrictHigh1 '0 0 1 0.5'  \
	 -gltCode corr_rememberedStrictHigh2 '0 0 1 -0.5'  \
