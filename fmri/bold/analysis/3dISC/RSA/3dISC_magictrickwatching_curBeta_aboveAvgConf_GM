3dISC -prefix ISC_magictrickwatching_curBeta_aboveAvgConf_GM -jobs 4 \
	 -model 'grp+curBeta_aboveAvgConf+grCurBeta_aboveAvgConf+(1|Subj1)+(1|Subj2)' \
	 -qVars   'grp,curBeta_aboveAvgConf,grCurBeta_aboveAvgConf' \
	 -qVarCenters   'curBeta_aboveAvgConf,grCurBeta_aboveAvgConf' \
	 -gltCode ave '1 0 0 0' \
	 -gltCode G11vG22 '0 1 0 0' \
	 -gltCode G11 '1 0.5 0 0' \
	 -gltCode G22 '1 -0.5 0 0' \
	 -gltCode curBeta_aboveAvgConf '0 0 1 0'  \
	 -gltCode curBeta_aboveAvgConf1vcurBeta_aboveAvgConf2 '0 0 0 1'\
	 -gltCode curBeta_aboveAvgConf1 '0 0 1 0.5'  \
	 -gltCode curBeta_aboveAvgConf2 '0 0 1 -0.5'  \
	 -mask /storage/shared/research/cinn/2018/MAGMOT/derivatives/ROI_masks/output/MNI_res-epi_label-GM_mask.nii.gz \
	 -dataTable @dataTable_magictrickwatching_memo.txt \