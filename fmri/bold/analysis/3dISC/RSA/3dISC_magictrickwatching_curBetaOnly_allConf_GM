3dISC -prefix ISC_magictrickwatching_curBetaOnly_allConf_GM -jobs 4 \
	 -model 'grp+curBetaOnly_allConf+grCurBetaOnly_allConf+(1|Subj1)+(1|Subj2)' \
	 -qVars   'grp,curBetaOnly_allConf,grCurBetaOnly_allConf' \
	 -qVarCenters   'curBetaOnly_allConf,grCurBetaOnly_allConf' \
	 -gltCode ave '1 0 0 0' \
	 -gltCode G11vG22 '0 1 0 0' \
	 -gltCode G11 '1 0.5 0 0' \
	 -gltCode G22 '1 -0.5 0 0' \
	 -gltCode curBetaOnly_allConf '0 0 1 0'  \
	 -gltCode curBetaOnly_allConf1vcurBetaOnly_allConf2 '0 0 0 1'\
	 -gltCode curBetaOnly_allConf1 '0 0 1 0.5'  \
	 -gltCode curBetaOnly_allConf2 '0 0 1 -0.5'  \
	 -mask /storage/shared/research/cinn/2018/MAGMOT/derivatives/ROI_masks/output/MNI_res-epi_label-GM_mask.nii.gz \
	 -dataTable @dataTable_magictrickwatching_memo.txt \