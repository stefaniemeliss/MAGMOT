	 -model 'grp+curBetaOnly_aboveAvgConf+grCurBetaOnly_aboveAvgConf+(1|Subj1)+(1|Subj2)' \
	 -qVars   'grp,curBetaOnly_aboveAvgConf,grCurBetaOnly_aboveAvgConf' \
	 -qVarCenters   'curBetaOnly_aboveAvgConf,grCurBetaOnly_aboveAvgConf' \
	 -gltCode ave '1 0 0 0' \
	 -gltCode G11vG22 '0 1 0 0' \
	 -gltCode G11 '1 0.5 0 0' \
	 -gltCode G22 '1 -0.5 0 0' \
	 -gltCode curBetaOnly_aboveAvgConf '0 0 1 0'  \
	 -gltCode curBetaOnly_aboveAvgConf1vcurBetaOnly_aboveAvgConf2 '0 0 0 1'\
	 -gltCode curBetaOnly_aboveAvgConf1 '0 0 1 0.5'  \
	 -gltCode curBetaOnly_aboveAvgConf2 '0 0 1 -0.5'  \
	 -mask /storage/shared/research/cinn/2018/MAGMOT/derivatives/ROI_masks/output/MNI_res-epi_label-GM_mask.nii.gz \
	 -dataTable @dataTable_magictrickwatching_memo.txt \