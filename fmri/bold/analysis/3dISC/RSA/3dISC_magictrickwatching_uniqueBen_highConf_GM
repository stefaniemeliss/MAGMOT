	 -model 'grp+uniqueBen_highConf+grUniqueBen_highConf+(1|Subj1)+(1|Subj2)' \
	 -qVars   'grp,uniqueBen_highConf,grUniqueBen_highConf' \
	 -qVarCenters   'uniqueBen_highConf,grUniqueBen_highConf' \
	 -gltCode ave '1 0 0 0' \
	 -gltCode G11vG22 '0 1 0 0' \
	 -gltCode G11 '1 0.5 0 0' \
	 -gltCode G22 '1 -0.5 0 0' \
	 -gltCode uniqueBen_highConf '0 0 1 0'  \
	 -gltCode uniqueBen_highConf1vuniqueBen_highConf2 '0 0 0 1'\
	 -gltCode uniqueBen_highConf1 '0 0 1 0.5'  \
	 -gltCode uniqueBen_highConf2 '0 0 1 -0.5'  \
	 -mask /storage/shared/research/cinn/2018/MAGMOT/derivatives/ROI_masks/output/MNI_res-epi_label-GM_mask.nii.gz \
