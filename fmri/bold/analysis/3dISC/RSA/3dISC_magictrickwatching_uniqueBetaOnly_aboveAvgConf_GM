	 -model 'grp+uniqueBetaOnly_aboveAvgConf+grUniqueBetaOnly_aboveAvgConf+(1|Subj1)+(1|Subj2)' \
	 -qVars   'grp,uniqueBetaOnly_aboveAvgConf,grUniqueBetaOnly_aboveAvgConf' \
	 -qVarCenters   'uniqueBetaOnly_aboveAvgConf,grUniqueBetaOnly_aboveAvgConf' \
	 -gltCode ave '1 0 0 0' \
	 -gltCode G11vG22 '0 1 0 0' \
	 -gltCode G11 '1 0.5 0 0' \
	 -gltCode G22 '1 -0.5 0 0' \
	 -gltCode uniqueBetaOnly_aboveAvgConf '0 0 1 0'  \
	 -gltCode uniqueBetaOnly_aboveAvgConf1vuniqueBetaOnly_aboveAvgConf2 '0 0 0 1'\
	 -gltCode uniqueBetaOnly_aboveAvgConf1 '0 0 1 0.5'  \
	 -gltCode uniqueBetaOnly_aboveAvgConf2 '0 0 1 -0.5'  \
	 -mask /storage/shared/research/cinn/2018/MAGMOT/derivatives/ROI_masks/output/MNI_res-epi_label-GM_mask.nii.gz \
	 -dataTable @dataTable_magictrickwatching_unique.txt \