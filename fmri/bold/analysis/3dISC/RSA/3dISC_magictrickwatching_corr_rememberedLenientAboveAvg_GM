	 -model 'grp+corr_rememberedLenientAboveAvg+grCorr_rememberedLenientAboveAvg+(1|Subj1)+(1|Subj2)' \
	 -qVars   'grp,corr_rememberedLenientAboveAvg,grCorr_rememberedLenientAboveAvg' \
	 -qVarCenters   'corr_rememberedLenientAboveAvg,grCorr_rememberedLenientAboveAvg' \
	 -gltCode ave '1 0 0 0' \
	 -gltCode G11vG22 '0 1 0 0' \
	 -gltCode G11 '1 0.5 0 0' \
	 -gltCode G22 '1 -0.5 0 0' \
	 -gltCode corr_rememberedLenientAboveAvg '0 0 1 0'  \
	 -gltCode corr_rememberedLenientAboveAvg1vcorr_rememberedLenientAboveAvg2 '0 0 0 1'\
	 -gltCode corr_rememberedLenientAboveAvg1 '0 0 1 0.5'  \
	 -gltCode corr_rememberedLenientAboveAvg2 '0 0 1 -0.5'  \
	 -mask /storage/shared/research/cinn/2018/MAGMOT/derivatives/ROI_masks/output/MNI_res-epi_label-GM_mask.nii.gz \
	 -dataTable @dataTable_magictrickwatching_memo.txt \